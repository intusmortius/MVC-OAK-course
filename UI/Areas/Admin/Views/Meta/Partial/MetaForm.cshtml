@model DTO.MetaDTO

<div class="box-body">
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        @Html.TextBoxFor(x => x.MetaContent, new { @class = "form-control", @placeholder = "Meta content" })
        @Html.ValidationMessageFor(x => x.MetaContent)
    </div>
    <div class="form-group">
        @Html.DropDownList("Name", new SelectList(new[] { "Keywords", "Description", "Author", "Twittercard" }), new { @class = "form-control select2" })
    </div>
    <input type="type" name="MetaID" value="@Model.MetaID" hidden />
    <div class="card-footer">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</div>
